{
  "name": "findoc-analyzer",
  "version": "1.0.0",
  "description": "Financial document analysis application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:pdf": "node run-tests.js",
    "test:all": "npm test && npm run test:pdf",
    "generate-report": "node generate-html-report.js",
    "predeploy": "node pre-deployment-check.js",
    "deploy": "gcloud app deploy app.yaml --quiet",
    "deploy:win": "deploy-to-gae.bat",
    "deploy:ps": "powershell -ExecutionPolicy Bypass -File .\\deploy-to-gae.ps1",
    "verify": "node verify-deployment.js",
    "generate-pdfs": "node generate-test-pdfs.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "python-shell": "^5.0.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
